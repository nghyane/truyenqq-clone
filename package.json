{
  "name": "quickstart",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run tw && bun --hot src/index.ts",
    "build": "bun run tw --minify && bun build src/index.ts",
    "start": "NODE_ENV=production bun src/index.ts",
    "install": "bun run src/install.ts",
    "tw": "bun tailwindcss -i ./src/base.css -o ./public/css/styles.css"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "tailwindcss": "^3.4.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@elysiajs/html": "^0.8.0",
    "@elysiajs/static": "^0.8.1",
    "@kitajs/ts-html-plugin": "^1.3.3",
    "@types/sequelize": "^4.28.19",
    "elysia": "^0.8.8",
    "sequelize": "^6.35.2"
  }
}